    <ui:InvokeCode ContinueOnError="{x:Null}" Code="        ' Specify the path to your Excel file&#xA;        Dim filePath As String = WorkbookPath&#xA;&#xA;        ' Load the workbook&#xA;        Using workbook As New XLWorkbook(filePath)&#xA;            ' Loop through each worksheet in the workbook&#xA;            For Each worksheet As IXLWorksheet In workbook.Worksheets&#xA;                ' Loop through each cell in the worksheet&#xA;                For Each cell As IXLCell In worksheet.CellsUsed()&#xA;                    ' Check if the cell has a hyperlink&#xA;                    If cell.HasHyperlink Then&#xA;                        ' Get the text of the hyperlink&#xA;                        Dim hyperlinkText As String = cell.GetFormattedString()&#xA;&#xA;                        ' Remove the hyperlink while keeping the text&#xA;                        cell.Hyperlink = Nothing&#xA;&#xA;                        ' Set the cell text to the original hyperlink text&#xA;                        cell.Value = hyperlinkText&#xA;                    End If&#xA;                Next&#xA;            Next&#xA;&#xA;            ' Save the modified workbook&#xA;            workbook.SaveAs(EditedWorkbookPath)&#xA;        End Using" DisplayName="Invoke Code" sap:VirtualizedContainerService.HintSize="450,245" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
